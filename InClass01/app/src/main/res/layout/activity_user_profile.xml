<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserProfile">


    <ImageView
        android:id="@+id/imageViewIDD"
        android:layout_width="182dp"
        android:layout_height="133dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/addpic" />

    <TextView
        android:id="@+id/firstEditIDD"
        android:layout_width="206dp"
        android:layout_height="32dp"
        android:layout_marginTop="46dp"
        android:ems="10"
        android:text="First Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewIDD" />

    <TextView
        android:id="@+id/lastEditIDD"
        android:layout_width="206dp"
        android:layout_height="32dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:text="Last Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstEditIDD" />

    <TextView
        android:id="@+id/textViewEmailIDD"
        android:layout_width="206dp"
        android:layout_height="32dp"
        android:layout_marginTop="30dp"
        android:text="Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lastEditIDD" />

    <TextView
        android:id="@+id/textViewGenderIDD"
        android:layout_width="206dp"
        android:layout_height="32dp"
        android:layout_marginTop="30dp"
        android:text="Gender"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEmailIDD" />

    <TextView
        android:id="@+id/cityEditIDD"
        android:layout_width="206dp"
        android:layout_height="32dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:text="City"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewGenderIDD" />

    <Button
        android:id="@+id/buttonBackIDD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityEditIDD" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:layout_marginTop="48dp"
        android:text="First Name"
        app:layout_constraintEnd_toStartOf="@+id/firstEditIDD"
        app:layout_constraintHorizontal_bias="0.343"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewIDD" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:layout_marginTop="44dp"
        android:text="Last Name"
        app:layout_constraintEnd_toStartOf="@+id/lastEditIDD"
        app:layout_constraintHorizontal_bias="0.354"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:layout_marginTop="44dp"
        android:text="Email "
        app:layout_constraintEnd_toStartOf="@+id/textViewEmailIDD"
        app:layout_constraintHorizontal_bias="0.354"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="44dp"
        android:text="Gender"
        app:layout_constraintEnd_toStartOf="@+id/textViewGenderIDD"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:layout_marginTop="44dp"
        android:text="City"
        app:layout_constraintEnd_toStartOf="@+id/cityEditIDD"
        app:layout_constraintHorizontal_bias="0.297"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

</androidx.constraintlayout.widget.ConstraintLayout>